<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox fx:id="loadTextWindow" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.igye.learnpl2.controllers.LoadTextController">
    <TextArea fx:id="textArea" prefHeight="346.0" prefWidth="600.0" wrapText="true" VBox.vgrow="ALWAYS"
            style="-fx-font-size: 20px"
            />
    <HBox spacing="10.0">
        <children>
            <Button fx:id="loadBtn" mnemonicParsing="false" text="Load" onAction="#loadButtonPressed"/>
            <Button fx:id="cancelBtn" mnemonicParsing="false" text="Cancel" onAction="#cancelButtonPressed" />
        </children>
        <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
        </padding>
    </HBox>
</VBox>
