<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox fx:id="rootNode" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="418.0" prefWidth="600.0" spacing="5.0" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1"
        fx:controller="org.igye.learnpl2.controllers.UserSettingsController">
    <children>
        <Label fx:id="settsLoadedFromLabel" text="Settings was loaded from:"/>
        <Label fx:id="settsLoadedFromPathLabel" text="FILE PATH"/>
        <Separator prefWidth="200.0"/>
        <Button fx:id="loadFromFileBtn" mnemonicParsing="false" text="Load from file"/>
        <Separator prefWidth="200.0"/>
        <GridPane prefHeight="70.0" prefWidth="600.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="294.0" minWidth="10.0" prefWidth="127.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="495.0" minWidth="10.0" prefWidth="473.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <Label text="_Directory with texts" mnemonicParsing="true" labelFor="${dirWithTextsTextField}">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </Label>
                <Label text="_URL for translation" GridPane.rowIndex="1" mnemonicParsing="true" labelFor="${urlTextField}">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </Label>
                <TextField fx:id="dirWithTextsTextField" GridPane.columnIndex="1">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </TextField>
                <TextField fx:id="urlTextField" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <GridPane.margin>
                        <Insets/>
                    </GridPane.margin>
                </TextField>
            </children>
        </GridPane>
        <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS"/>
        <Separator prefWidth="200.0"/>
        <HBox alignment="CENTER_RIGHT" prefHeight="1.0" prefWidth="570.0" spacing="15.0">
            <children>
                <Button fx:id="closeBtn" mnemonicParsing="false" text="Close"/>
                <Button fx:id="saveAsBtn" mnemonicParsing="false" text="Save as"/>
                <Button fx:id="saveBtn" mnemonicParsing="false" text="Save"/>
            </children>
            <padding>
                <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
            </padding>
        </HBox>
    </children>
    <padding>
        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
    </padding>
</VBox>
