<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<StackPane fx:id="rootNode" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="org.igye.learnpl2.controllers.MainWindowController">
    <HBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
          prefHeight="400.0" prefWidth="600.0" spacing="50.0">
        <children>
            <Button fx:id="textBtn" mnemonicParsing="false" text="Text"/>
            <Button fx:id="cardsBtn" mnemonicParsing="false" text="Cards"/>
        </children>
    </HBox>
</StackPane>